{
    "name": "ipsis/fornecedores-cadastro",
    "description": "Sistema de cadastro de fornecedores com integração Google Sheets",
    "type": "project",
    "require": {
        "php": ">=7.4",
        "google/apiclient": "^2.15",
        "vlucas/phpdotenv": "^5.5",
        "setasign/fpdf": "^1.8",
        "setasign/fpdi": "^2.3"
    },
    "autoload": {
        "psr-4": {
            "Ipsis\\": "src/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
